{
  "Content-Security-Policy": "default-src 'self'; script-src 'self' 'unsafe-inline' https://trusted-cdn.com; style-src 'self' 'unsafe-inline'; img-src 'self' data: https://trusted-image-source.com; font-src 'self' https://trusted-fonts.com;",
  "Strict-Transport-Security": "max-age=31536000; includeSubDomains; preload",
  "X-Frame-Options": "DENY",
  "X-Content-Type-Options": "nosniff",
  "Referrer-Policy": "strict-origin-when-cross-origin",
  "Permissions-Policy": "geolocation=(), microphone=(), camera=()",
  "Cross-Origin-Opener-Policy": "same-origin",
  "Cross-Origin-Embedder-Policy": "require-corp",
  "Cross-Origin-Resource-Policy": "same-origin",
  "Expect-CT": "enforce, max-age=86400",
  "Cache-Control": "public, max-age=31536000, immutable",
  "Feature-Policy": "geolocation 'none'; microphone 'none'; camera 'none'; payment 'self';"
}
